{
    "name": "loki1729/laravel-thepeer",
    "description": "Laravel Package for easy integration of ThePeer APIs.",
    "keywords": ["http", "rest", "web service", "thepeer", "Thepeer", "laravel Thepeer"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Loki1729\\LaravelThePeer\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Olarewaju Mojeed",
            "email": "olarewajumojeed9@gmail.com"
        }
    ],
    "require": {
        "php" : ">=8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.6"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Loki1729\\LaravelThePeer\\ServiceProviders\\ThePeerServiceProvider"
            ],
            "aliases": {
                "ThePeer": "Loki1729\\LaravelThePeer\\Facades\\ThePeer"
            }
        }
    }
}
